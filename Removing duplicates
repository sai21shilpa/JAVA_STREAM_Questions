import java.util.Arrays;
import java.util.List;
import java.util.stream.Collectors;


public class StreamClass{
 public static void main(String args[])
 {
    List<String> listOfString=Arrays.asList("Shilpa","Sai","Komma","Shilpa");
   //reading the list of the string
    List<String> resList=listOfString.stream().distinct().collect(Collectors.toList());
   //reading the list then streaming the data, later making it distinct ( non-repeated) later collecting it to be formed as the List
   System.out.print(resList);
 }
}
 
